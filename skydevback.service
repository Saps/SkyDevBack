[Unit]
Description=Background module for skydev
After=network.target

[Service]
User=root
Group=root
WorkingDirectory=/var/www/SkyDevBack
Environment=""
ExecStart=python services.py
ExecReload=/bin/kill -s HUP $MAINPID
KillMode=mixed
TimeoutStopSec=5
PrivateTmp=true

[Install]
WantedBy=multi-user.target